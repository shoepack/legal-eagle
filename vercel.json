{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/highlight.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/python-api/highlight",
      "dest": "/api/highlight.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
