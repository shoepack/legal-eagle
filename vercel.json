{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/highlight.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/python-api/highlight",
      "destination": "/api/highlight.py"
    },
    {
      "source": "/python-api/debug/versions",
      "destination": "/api/highlight.py"
    }
  ]
}
